<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane onMouseClicked="#cardClicked" prefHeight="118.0" prefWidth="318.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111">
   <children>
      <ImageView fx:id="cardImage" fitHeight="118.0" fitWidth="101.0" pickOnBounds="true">
         <image>
            <Image url="@../icons/novisad.jpg" />
         </image>
      </ImageView>
      <Label fx:id="cardLabel" layoutX="106.0" layoutY="4.0" text="Super ekstra tura">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="109.0" layoutY="35.0" text="city:" />
      <Label fx:id="cityName" layoutX="135.0" layoutY="33.0" text="Novi Sad">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="106.0" layoutY="51.0" text="type:" />
      <Label fx:id="tourType" layoutX="134.0" layoutY="51.0" text="walking(dead)">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="103.0" layoutY="68.0" text="spots:" />
      <Label fx:id="freeSpots" layoutX="135.0" layoutY="67.0" text="5">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <ImageView fx:id="guidePhoto" fitHeight="50.0" fitWidth="31.0" layoutX="281.0" layoutY="62.0" pickOnBounds="true">
         <image>
            <Image url="@../icons/user.jpg" />
         </image>
      </ImageView>
      <Label layoutX="280.0" layoutY="45.0" text="Guide:" />
      <Label fx:id="guideUsername" alignment="CENTER_RIGHT" layoutX="197.0" layoutY="96.0" prefHeight="17.0" prefWidth="81.0" text="motomaniac96" />
   </children>
</AnchorPane>
