<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="labelStatus" layoutX="46.0" layoutY="111.0" prefHeight="17.0" prefWidth="142.0" textFill="#1eacbf" />
      <PasswordField fx:id="password" focusTraversable="false" layoutX="48.0" layoutY="76.0" promptText="Password" style="-fx-border-color: #1e90ff;" />
      <Button contentDisplay="CENTER" layoutX="48.0" layoutY="135.0" mnemonicParsing="false" onAction="#logIn" prefHeight="32.0" prefWidth="67.0" style="-fx-border-color: #1e90ff;" text="Log in" textAlignment="CENTER" textFill="DODGERBLUE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="loginCancelButton" contentDisplay="CENTER" layoutX="132.0" layoutY="135.0" mnemonicParsing="false" onAction="#loginCanceled" prefHeight="32.0" prefWidth="67.0" style="-fx-border-color: #1e90ff;" text="Cancel" textAlignment="CENTER" textFill="DODGERBLUE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button contentDisplay="CENTER" layoutX="345.0" layoutY="305.0" mnemonicParsing="false" onAction="#signUpClicked" prefHeight="32.0" prefWidth="73.0" style="-fx-border-color: #1e90ff;" text="Sign up" textAlignment="CENTER" textFill="DODGERBLUE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="329.0" layoutY="226.0" prefHeight="27.0" prefWidth="105.0" text="New to MiTe?" textFill="DODGERBLUE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="username" focusTraversable="false" layoutX="49.0" layoutY="35.0" promptText="Username" style="-fx-border-color: #1e90ff;" />

   </children>
</AnchorPane>
